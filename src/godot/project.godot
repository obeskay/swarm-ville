; Engine configuration for SwarmVille
; https://docs.godotengine.org/en/stable/tutorials/misc/overriding_build_variables.html

config_version=5

[application]
config/name="SwarmVille"
config/version="1.0.0"
config/description="Multiplayer game platform with Godot 4.5 and Rust backend"
run/main_scene="res://scenes/main/main.tscn"

[autoload]
NetworkManager="res://scripts/network/network_manager.gd"
SpaceManager="res://scripts/managers/space_manager.gd"
AgentManager="res://scripts/managers/agent_manager.gd"

[display]
window/size/viewport_width=1280
window/size/viewport_height=720
window/size/window_width_override=1280
window/size/window_height_override=720

[physics]
2d/default_gravity=0

[rendering]
renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
textures/vram_compression/import_etc2_astc=true
