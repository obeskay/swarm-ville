{
  "productName": "SwarmVille",
  "version": "0.1.0",
  "identifier": "com.swarmville.app",
  "build": {
    "beforeBuildCommand": "pnpm run build:godot",
    "beforeDevCommand": "pnpm run build:godot",
    "devUrl": "http://localhost:8000",
    "frontendDist": "../godot_build"
  },
  "app": {
    "windows": [
      {
        "title": "SwarmVille",
        "width": 1280,
        "height": 720,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {}
}
